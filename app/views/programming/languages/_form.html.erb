<%= render 'shared/error_messages', object: f.object %>

<div class="control-group">
	<%= f.label :name, class: 'control-label' %>
	<div class="controls">
		<%= f.text_field :name, class: 'input-xlarge' %>
	</div>
</div>
<div class="control-group">
	<%= f.label :compiler, 'Compiler Command', class: 'control-label' %>
	<div class="controls">
		<%= f.text_field :compiler, class: 'input-xlarge' %>
	</div>
</div>
<div class="control-group">
	<%= f.label :execution, 'Execution Command', class: 'control-label' %>
	<div class="controls">
		<%= f.text_field :execution, class: 'input-xlarge' %>
	</div>
</div>
<div class="control-group">
	<%= f.label :icon, 'Icon File Path', class: 'control-label' %>
	<div class="controls">
		<%= f.text_field :icon, class: 'input-xlarge' %>
	</div>
</div>
<div class="control-group">
	<div class="controls">
		<%= f.check_box :available, class: 'pull-left' %>
		<%= f.label :available, class: 'checkbox' %>
	</div>
</div>
